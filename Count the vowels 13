#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include <ctype.h>
static int countVowels(const char *word) {
    int count = 0;
    for (int i = 0; word[i] != '\0'; i++) {
        char ch = tolower(word[i]); 
        if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
            count++;
        }
    }
    return count;
}
int main() {
    char inputWord[100]; 
    scanf("%s", inputWord); 
    int vowelCount = countVowels(inputWord);
    printf("count: %d\n", vowelCount);
    return 0;
}
