#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,count=0;
    scanf("%d",&a);
    if(a>=1&&a<=10000000)
    {
        int temp=a;
        while(temp>0)
        {
            temp/=10;
            count++;
        }
        printf("The count of the given integer is: %d\n",count);
    }
    else
    {
        printf("Enter a Valid Input");
    }
    return 0;
}
