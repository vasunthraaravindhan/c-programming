#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d", &n);
    int arr[n];
    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }
    double sum = 0;
    for(int i = 0; i < n; i++) {
        sum += arr[i];
    }
    double mean = sum / n;
    printf("Array Mean Value is %.2f\n", mean);
    return 0;
}
