#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    int num; 
    scanf("%d", &num); 
    if (num >= 1 && num <= 9) {
        for (int i = 1; i <= 5; i++) { 
            printf("%d x %d = %d\n", num, i, num * i); 
        }
    } else {
        printf("Invalid Input\n"); 
    }
    return 0;
}
