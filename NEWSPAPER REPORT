#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,c;
    scanf("%d",&a);
    scanf("%d",&b);
    scanf("%d",&c);
    printf("TSUNAMI REPORT OF JAPAN\n");
    printf("The number of people\n");
    printf("1)Dead:%d\n",a);
    printf("2)Injured:%d\n",b);
    printf("3)Safe:%d\n",c);
    printf("Please help the people who are suffering!!!\n");
    return 0;
}
