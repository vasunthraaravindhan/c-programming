#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num, temp, sum = 0;
    scanf("%d", &num);
    temp = num;
    while(temp > 0) {
        sum += temp % 10;
        temp /= 10;
    }
    if(num % sum == 0)
        printf("Harshad Number");
    else
        printf("Not Harshad Number");
    return 0;
}
