#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int x1, y1, x2, y2;
    float midX, midY;
    scanf("%d %d %d %d", &x1, &y1, &x2, &y2);
    midX = (x1 + x2) / 2.0;
    midY = (y1 + y2) / 2.0;
    printf("Binoy's house is located at (%.1f,%.1f)", midX, midY);
    return 0;
}
